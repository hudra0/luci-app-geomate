{
    "luci-app-geomate": {
        "description": "Grant access to Geomate configuration",
        "read": {
            "ubus": {
                "luci": [ "getInitList", "setInitAction" ],
                "luci.geomate": [ "getGeomateConnections", "getAllowedIPs", "getStatus", "getHealthCheck", "getGeolocationStatus" ],
                "uci": [ "get", "state" ]
            },
            "uci": [ "geomate" ],
            "file": {
                "/etc/init.d/geomate": [ "read", "exec" ],
                "/etc/geolocate.sh": [ "read", "exec" ],
                "/var/run/geomate.pid": [ "read" ],
                "/etc/geomate.d/*": [ "read" ],
                "/etc/geomate.d/runtime/*": [ "read" ],
                "/tmp/geomate_nft_errors.log": [ "read" ]
            }
        },
        "write": {
            "ubus": {
                "luci": [ "setInitAction" ],
                "luci.geomate": [ "getGeomateConnections", "getAllowedIPs", "getStatus", "getHealthCheck", "getGeolocationStatus" ],
                "uci": [ "set", "commit", "apply" ]
            },
            "uci": [ "geomate" ],
            "file": {
                "/etc/geomate.d/*": [ "write" ]
            }
        }
    }
}
